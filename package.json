{
  "name": "dvote-js",
  "version": "0.16.2",
  "description": "Javascript/Typescript library to interact with Vocdoni voting processes",
  "main": "dist/index",
  "types": "dist/index",
  "repository": "https://github.com/vocdoni/dvote-js.git",
  "author": "Vocdoni <root@vocdoni.io>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rm -fR dist",
    "build": "npm run clean && ./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc -w -p .",
    "ts-node": "./node_modules/.bin/ts-node",
    "test": "npm run build && ./node_modules/.bin/mocha -r ts-node/register test/**/*.ts",
    "test:unit": "npm run build && ./node_modules/.bin/mocha -r ts-node/register test/unit/*.ts",
    "test:integration": "npm run build && ./node_modules/.bin/mocha -r ts-node/register test/integration/*.ts",
    "postinstall": "rm -Rf node_modules/ganache-core/node_modules/web3-providers-ws/node_modules/websocket/.git || true"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "buffer": "^5.2.1",
    "circomlib": "^0.1.2",
    "dvote-solidity": "0.5.2-xdai",
    "ethers": "^4.0.47",
    "iso-language-codes": "^1.0.6",
    "isomorphic-ws": "^4.0.1",
    "js-sha3": "^0.8.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-sealedbox-js": "^1.2.0",
    "universal-parse-url": "^1.0.2",
    "ws": "^7.1.0",
    "yup": "^0.29.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^5.0.7",
    "@types/ws": "^6.0.1",
    "@types/yup": "^0.29.0",
    "chai": "^4.2.0",
    "dotenv": "^7.0.0",
    "ganache-cli": "^6.4.5",
    "ganache-core": "^2.6.1",
    "mocha": "^5.2.0",
    "sinon": "^7.3.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "^3.7.3"
  }
}
