{
  "name": "dvote-js",
  "version": "1.3.1",
  "description": "Javascript/Typescript library to interact with Vocdoni voting processes",
  "main": "dist/index",
  "types": "dist/index",
  "repository": "https://github.com/vocdoni/dvote-js.git",
  "author": "Vocdoni <root@vocdoni.io>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "prebuild": "git submodule init && git submodule update",
    "build": "npm run clean && ./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc -w -p .",
    "ts-node": "./node_modules/.bin/ts-node",
    "test": "npm run build && ./node_modules/.bin/mocha -r ts-node/register test/**/*.ts",
    "test:unit": "npm run build && ./node_modules/.bin/mocha -r ts-node/register test/unit/*.ts",
    "test:integration": "npm run build && ./node_modules/.bin/mocha -r ts-node/register test/integration/*.ts",
    "postinstall": "rimraf node_modules/ganache-core/node_modules/web3-providers-ws/node_modules/websocket/.git || true"
  },
  "dependencies": {
    "@vocdoni/storage-proofs-eth": "^0.2.3",
    "axios": "^0.21.1",
    "blindsecp256k1": "0.0.6",
    "buffer": "^6.0.3",
    "circomlib": "^0.1.2",
    "dvote-solidity": "^1.3.1",
    "ethers": "^5.0.23",
    "google-protobuf": "^3.14.0",
    "iso-language-codes": "^1.0.6",
    "js-sha3": "^0.8.0",
    "long": "^4.0.0",
    "protobufjs": "^6.10.2",
    "tweetnacl": "^1.0.3",
    "tweetnacl-sealedbox-js": "^1.2.0",
    "universal-parse-url": "^1.0.2",
    "ws": "^7.1.0",
    "yup": "^0.31.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/elliptic": "^6.4.12",
    "@types/express": "^4.17.9",
    "@types/google-protobuf": "^3.7.4",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^5.0.7",
    "@types/ws": "^6.0.1",
    "@types/yup": "^0.29.0",
    "chai": "^4.2.0",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "ganache-cli": "^6.4.5",
    "ganache-core": "^2.6.1",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^7.3.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "^3.7.3"
  }
}
